        <header class="post-header"><h1>{{title}}</h1></header>

        <div class="post-content">
            {{content}}
        </div>

    </article>
    <section class="comments">

<a class="moot"
   href="https://moot.it/i/oct-zoy-org/discussions/{{id}}"
   data-pageTitle="{{title}}"
   data-show_title="true"
   data-show_online="true">
  Commentaires
</a>
{{/post}}
